{
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": false,
  "importOrder": [
    "^react",
    "^(?!(types|utils|components|modules|[./])).+",
    "^types",
    "^utils",
    "^components",
    "^containers",
    "^modules",
    "^[./]"
  ],
  "importOrderSeparation": true
}
